{
  "name": "planncomm-frontend",
  "version": 2,
  "builds": [
    {
      "src": "planncomm-frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/planncomm-frontend/build/$1"
    }
  ]
}
